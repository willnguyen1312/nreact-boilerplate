{
  "name": "nreact-boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reach/router": "^1.2.1",
    "@types/jest": "24.0.11",
    "@types/node": "11.13.0",
    "@types/react": "16.8.10",
    "@types/react-dom": "16.8.3",
    "apollo-boost": "^0.3.1",
    "axios": "^0.18.0",
    "axios-extensions": "^3.0.5",
    "graphql": "^14.2.1",
    "graphql-tag": "^2.10.1",
    "react": "^16.8.6",
    "react-apollo": "^2.5.3",
    "react-dom": "^16.8.6",
    "react-scripts": "2.1.8",
    "rebass": "^3.1.0",
    "styled-components": "^4.2.0",
    "typescript": "^3.4.1"
  },
  "scripts": {
    "start": "cross-env NODE_PATH=./src react-scripts start",
    "build": "cross-env NODE_PATH=./src react-scripts build",
    "test:unit": "react-scripts test --modulePaths=src",
    "test:unit:ci": "CI=true react-scripts test --modulePaths=src --coverage",
    "test:e2e": "cypress open",
    "test:e2e:ci": "start-server-and-test start http://localhost:3000 cy:run",
    "cy:run": "cypress run",
    "lint": "tslint --project tsconfig.json",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@storybook/addon-actions": "^4.1.11",
    "@storybook/addon-info": "^4.1.11",
    "@storybook/addon-links": "^4.1.11",
    "@storybook/addons": "^4.1.11",
    "@storybook/react": "^4.1.11",
    "@types/reach__router": "^1.2.3",
    "@types/rebass": "^3.0.3",
    "@types/storybook__react": "^4.0.1",
    "@types/styled-components": "^4.1.12",
    "@types/webpack-env": "^1.13.9",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.2.0",
    "cypress": "^3.2.0",
    "cypress-testing-library": "^2.3.6",
    "husky": "^1.3.1",
    "jest-dom": "^3.1.3",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "react-docgen-typescript-loader": "^3.0.1",
    "react-testing-library": "^6.0.4",
    "start-server-and-test": "^1.7.13",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "waait": "^1.0.4"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "resolutions": {
    "@types/react": "16.8.10"
  }
}
